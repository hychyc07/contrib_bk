		<protocol>udp</protocol>
	</connection>
	<connection>
        	<from>/eventFeaHor/feaRight:o</from>
		<to>/rightFeaHor</to>
		<protocol>udp</protocol>
	</connection>
    	<connection>
        	<from>/eventFeaVer/feaLeft:o</from>
		<to>/leftFeaVer</to>
		<protocol>udp</protocol>
	</connection>
	<connection>
        	<from>/eventFeaVer/feaRight:o</from>
		<to>/rightFeaVer</to>
		<protocol>udp</protocol>
	</connection>
    	<connection>
        	<from>/eventFeaPos/feaLeft:o</from>
		<to>/leftFeaPos</to>
		<protocol>udp</protocol>
	</connection>
	<connection>
        	<from>/eventFeaPos/feaRight:o</from>
		<to>/rightFeaPos</to>
		<protocol>udp</protocol>
	</connection>
    	<connection>
        	<from>/eventFeaNeg/feaLeft:o</from>
		<to>/leftFeaNeg</to>
		<protocol>udp</protocol>
	</connection>
	<connection>
        	<from>/eventFeaNeg/feaRight:o</from>
		<to>/rightFeaNeg</to>
		<protocol>udp</protocol>
	</connection>
	<connection>
        	<from>/eventFeaHor/eventBottle:o</from>
		<to>/eventSelectiveAttention/retina:i</to>
		<protocol>udp</protocol>
	</connection>
	<connection>
        	<from>/eventFeaVer/eventBottle:o</from>
		<to>/eventSelectiveAttention/retina2:i</to>
		<protocol>udp</protocol>
	</connection>
	<connection>
        	<from>/eventFeaPos/eventBottle:o</from>
		<to>/eventSelectiveAttention/retina3:i</to>
		<protocol>udp</protocol>
	</connection>
	<connection>
        	<from>/eventFeaNeg/eventBottle:o</from>
		<to>/eventSelectiveAttention/retina4:i</to>
		<protocol>udp</protocol>
	</connection>
	<connection>
        	<from>/eventSelectiveAttention/left:o</from>
		<to>/saliencyMapLeft</to>
		<protocol>udp</protocol>
	</connection>
    <connection>
        	<from>/velocityExtractor/plotter/histo:o</from>
		<to>/velHistoView</to>
		<protocol>udp</protocol>
	</connection>
    <connection>
        	<from>/velocityExtractor/plotter/vel:o</from>
		<to>/velExtractorView</to>
		<protocol>udp</protocol>
	</connection>
    <connection>
        	<from>/OptFlViewer/vels:o</from>
		<to>/velocityExtractor/plotter/vel:i</to>
		<protocol>udp</protocol>
	</connection>
        <connection>
        	<from>/flow/opticalFlow:o</from>
		<to>/velocityExtractor/velocity:i</to>
		<protocol>udp</protocol>
	</connection>
</application>
