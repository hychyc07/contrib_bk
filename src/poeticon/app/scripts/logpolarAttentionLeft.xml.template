<application>	<name>AttentionMechanism Left Drive </name> 	<dependencies>		<port>/icub/cam/left</port>		<port>/logPolarTransform/icub/left_cam/image:o</port>
        <port>/vergence/cmd:o</port>	</dependencies>	<module>		<name>visualFilter</name>		<parameters>--name /visualFilter/icub/left_cam</parameters>		<tag>vFilter</tag>		<node>/icub-b12</node>	</module>	<module>		<name>yuvProcessor</name>		<parameters>--name yuvProc/icub/left_cam</parameters>		<tag>yuvProcessorLeft</tag>		<node>/icub-b12</node>	</module>	<module>		<name>yuvProcessor</name>		<parameters>--name hsvProc/icub/left_cam --image HSV</parameters>		<tag>yuvProcessorLeftHSV</tag>		<node>/icub-b12</node>	</module>	<module>		<name>saliencyBlobFinder</name>		<parameters>--from saliencyBlobFinderLeft.ini --config icubEyes.ini --context logpolarAttention/conf</parameters>		<tag>saliencyBlobFinderLeft</tag>		<node>/icub-b12</node>	</module>	<module>		<name>selectiveAttentionEngine</name>		<parameters>--from selectiveAttentionLeft.ini --context logpolarAttention/conf </parameters>		<tag>selectiveAttLeft</tag>		<node>/icub-b12</node>	</module>    
    <module>
        <name>gazeArbiter</name>
        <parameters>--name /gazeArbiter/icub/left_cam --config icubEyes.ini --ymin -0.25 --ymax 0.25 --xmin -0.60 --xmax -0.3 --blockPitch -40.0</parameters>
        <tag>gArbiter</tag>
        <node>/icub-b12</node>
    </module>	<connection>        <from>/logPolarTransform/icub/left_cam/image:o</from>		<to>/visualFilter/icub/left_cam/image:i</to>		<protocol>udp</protocol>	</connection>    <connection>        <from>/logPolarTransform/icub/left_cam/image:o</from>		<to>/yuvProc/icub/left_cam/image:i</to>		<protocol>udp</protocol>	</connection>	<connection>        <from>/logPolarTransform/icub/left_cam/image:o</from>		<to>/hsvProc/icub/left_cam/image:i</to>		<protocol>udp</protocol>	</connection>	<connection>      	<from>/logPolarTransform/icub/left_cam/image:o</from>		<to>/blobFinder/icub/left_cam/image:i</to>		<protocol>udp</protocol>	</connection>	<connection>      	<from>/visualFilter/icub/left_cam/rg:o</from>		<to>/blobFinder/icub/left_cam/rg:i</to>		<protocol>shmem</protocol>	</connection>	<connection>        <from>/visualFilter/icub/left_cam/gr:o</from>		<to>/blobFinder/icub/left_cam/gr:i</to>		<protocol>shmem</protocol>	</connection>	<connection>        <from>/visualFilter/icub/left_cam/by:o</from>		<to>/blobFinder/icub/left_cam/by:i</to>		<protocol>shmem</protocol>	</connection>	<connection>        <from>/visualFilter/icub/left_cam/image:o</from>		<to>/blobFinder/icub/left_cam/edges:i</to>		<protocol>shmem</protocol>	</connection>		<connection>        <from>/motionCUT/left/opt:o</from>		<to>/selectiveAttentionEngine/icub/left_cam/motion:i</to>		<protocol>udp</protocol>	</connection>		<connection>        <from>/blobFinder/icub/left_cam/salienceMap:o</from>		<to>/selectiveAttentionEngine/icub/left_cam/map4:i</to>		<protocol>shmem</protocol>	</connection>	<connection>        <from>/hsvProc/icub/left_cam/H/image:o</from>		<to>/selectiveAttentionEngine/icub/left_cam/map1:i</to>		<protocol>shmem</protocol>	</connection>	<connection>        <from>/hsvProc/icub/left_cam/S/image:o</from>		<to>/selectiveAttentionEngine/icub/left_cam/map2:i</to>		<protocol>shmem</protocol>	</connection>	<connection>        <from>/hsvProc/icub/left_cam/V/image:o</from>		<to>/selectiveAttentionEngine/icub/left_cam/map3:i</to>		<protocol>shmem</protocol>	</connection>	<connection>        <from>/yuvProc/icub/left_cam/UV/image:o</from>		<to>/selectiveAttentionEngine/icub/left_cam/map6:i</to>		<protocol>shmem</protocol>	</connection>    </application>
